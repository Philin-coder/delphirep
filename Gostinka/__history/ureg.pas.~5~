    ComboBox1: TComboBox;
    Label2: TLabel;
    Edit1: TEdit;
    Panel1: TPanel;
    Button1: TButton;
    procedure Button1Click(Sender: TObject);
  private
    { Private declarations }
  public
    { Public declarations }
  end;

var
  Freg: TFreg;

implementation

uses Umain;

{$R *.dfm}

procedure TFreg.Button1Click(Sender: TObject);
begin
case
ComboBox1.ItemIndex of
0:
begin
ShowMessage(IntToStr(ComboBox1.ItemIndex));
end;
1:
begin
ShowMessage(IntToStr(ComboBox1.ItemIndex));
end;
2:
begin
ShowMessage(IntToStr(ComboBox1.ItemIndex));
end;

end;//case
end;

end.
