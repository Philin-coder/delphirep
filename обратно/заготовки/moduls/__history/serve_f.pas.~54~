unit serve_f;

interface
type mx=array[1..155] of Byte;
var n:Integer;
Procedure get_rnd(n:Integer);
implementation

uses fmain,SysUtils;
Procedure get_rnd(n:Integer);
var x:mx;
var i:Integer;
begin
for I:=1 to n do
begin
Randomize;
x[i]:=Random(24)+97;
end;
for I := 1 to n do
begin
//if (x[i]>97)and (x[i]<122) then

//fmain.Form1.LabeledEdit1.Text:=fmain.Form1.LabeledEdit1.Text+' '+IntToStr(x[i]);
fmain.Form1.LabeledEdit1.Text:=fmain.Form1.LabeledEdit1.Text+' '+chr(x[i]));

end;
//Random(delta) + base
//где:
//delta = (20-15) + 1 = 6
//base = 15
//    15 до 20:
//97-122
end.
