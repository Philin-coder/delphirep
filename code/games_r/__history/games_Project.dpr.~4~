{$R 'resources.RES'}

begin
  Application.Initialize;
  Application.CreateForm(TFrm_main, Frm_main);
  Application.CreateForm(TDM, DM);
  Application.CreateForm(TFrm_work, Frm_work);
  Application.CreateForm(TFrm_etap, Frm_etap);
  Application.Run;
end.
