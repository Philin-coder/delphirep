unit Unmain;

interface

uses
  Windows, Messages, SysUtils, Variants, Classes, Graphics, Controls, Forms,jpeg,
  Dialogs;

type
  TFrmMain = class(TForm)
    procedure FormActivate(Sender: TObject);
  private
    { Private declarations }
  public
    { Public declarations }
  end;

var
  FrmMain: TFrmMain;

implementation

{$R *.dfm}

procedure TFrmMain.FormActivate(Sender: TObject);
begin
with FrmMain do
begin
  width:=1024;
  height:=768;
end;
end;

end.
