unit Un_man;

interface

uses
  Windows, Messages, SysUtils, Variants, Classes, Graphics, Controls, Forms,
  Dialogs,jpeg, Menus, ExtCtrls;

type
  TFrm_main = class(TForm)
    ItemMenu: TMainMenu;
    good_item: TMenuItem;
    mainImage: TImage;
    procedure FormCreate(Sender: TObject);
  private
    { Private declarations }
  public
    { Public declarations }
  end;

var
  Frm_main: TFrm_main;

implementation

uses Un_func;

{$R *.dfm}

procedure TFrm_main.FormCreate(Sender: TObject);
begin
SetFormPropertiesIfNeeded;
// Закрытие всех запросов на форме 'dm'
 // CloseAllQueriesOnForm('dm'); вызов
 with mainImage do
 begin
     Stretch:=True;
     AutoSize:=True;
 end;
end;

end.
